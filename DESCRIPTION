Package: xmeta
Type: Package
Title: A Toolbox for Multivariate Meta-Analysis
Version: 1.1-5
Date: 2019-10-30
Authors@R: c(person("Yong", "Chen", role=c("aut", "cre"),
                     email ="ychen123@upenn.edu"),
		     person("Chuan", "Hong", role="aut"),
			 person("Haitao", "Chu", role="ctb"),
